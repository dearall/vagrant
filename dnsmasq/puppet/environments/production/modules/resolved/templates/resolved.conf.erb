[Resolve]
<% if @dns then -%>
DNS=<%= @dns -%>
<% else %>
#DNS=
<%- end -%>
<%- if @fallback_dns then -%>
FallbackDNS=<%= @fallback_dns -%>
<% else %>
#FallbackDNS=
<% end -%>
<%- if @domains then -%>
Domains=<%= @domains -%>
<% else -%>
#Domains=
<% end %>
LLMNR=<%= @llmnr %>
MulticastDNS=<%= @multicast_dns %>
DNSSEC=<%= @dnssec %>
DNSOverTLS=<%= @dns_over_tls %>
Cache=<%= @cache %>
DNSStubListener=<%= @dns_stub_listener %>
ReadEtcHosts=<%= @read_etc_hosts%>
